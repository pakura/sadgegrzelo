{
    "name": "pakura/stella-cms",
    "type": "project",
    "description": "Content Management System based on Laravel Framework.",
    "keywords": ["cms", "framework", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vaja Sinauridze",
            "email": "vajasin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "barryvdh/laravel-elfinder": "0.4.*",
        "cocur/slugify": "4.0.*",
        "laravelcollective/html": "6.1.*",
        "league/glide": "1.6.*",
        "mews/captcha": "3.1.*",
        "php-curl-class/php-curl-class": "8.8.*",
        "ext-dom": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
